@namespace("com.yshi.avro")
protocol YibingProtocol{
    record TestRecord {
        string recordId;
        long longField = -1;
        int intField = 1;
        date dateField = "2016-01-01";
        @logicalType("decimal") @precision(7) @scale(2) bytes decimalType = "\u0000000";
    }
    enum Status {
        OK, FAILED
    }

    Status send (TestRecord request);
}
